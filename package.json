{
  "name": "jsonresume-theme-react",
  "version": "0.0.1",
  "description": "A React-based theme for JSON Resume",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev:base": "vite",
    "dev": "node scripts/dev.js",
    "build:base": "tsc -b && vite build",
    "build": "node scripts/build.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "postinstall": "node scripts/generateTypes.js && npm run build:base"
  },
  "keywords": [
    "jsonresume",
    "jsonresume-theme",
    "resume",
    "cv",
    "react",
    "theme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/phoinixi/jsonresume-theme-react.git"
  },
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@jsonresume/schema": "1.2.1",
    "clsx": "2.1.1",
    "date-fns": "^4.1.0",
    "i18next": "^24.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.4.1",
    "react-icons": "5.5.0",
    "react-markdown": "10.1.0",
    "remark-gfm": "4.0.1",
    "tailwind-merge": "3.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@iconify/react": "^5.2.0",
    "@iconify/tailwind": "^1.2.0",
    "@types/i18next": "^12.1.0",
    "@types/node": "^22.13.13",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-i18next": "^7.8.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "json-schema-to-typescript": "^15.0.4",
    "minimist": "^1.2.8",
    "postcss": "^8.5.3",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
