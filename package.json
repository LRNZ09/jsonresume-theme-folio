{
	"name": "jsonresume-theme-folio",
	"version": "0.1.0",
	"keywords": [
		"jsonresume",
		"jsonresume-theme",
		"resume",
		"cv",
		"react",
		"theme"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com:LRNZ09/jsonresume-theme-folio.git"
	},
	"license": "MIT",
	"author": "Lorenzo Pieri",
	"type": "module",
	"scripts": {
		"build": "bun run build.ts",
